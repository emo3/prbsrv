## Define the Process Agent(s)
# Object Server Process Agent
[<%= node['prbsrv']['os_pa_name'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['pa_port'] %>
}

# Probe Server Process Agent
[<%= node['prbsrv']['ps_pa_name'] %>]
{
  Primary: <%= node['PS'] %> <%= node['prbsrv']['pa_port'] %>
}

## Define the Object Server(s)
# Object Server
[<%= node['prbsrv']['ncoms'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['os_port'] %>
}

# Primary Object Server
[<%= node['prbsrv']['p-ncoms'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['os_port'] %>
}

# Backup Object Server
[<%= node['prbsrv']['b-ncoms'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['os_port'] %>
}

# Define the Gateway(s)
# Bi Directional Gateway to connect to Backup Object Server
[<%= node['prbsrv']['bi-name'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['bi_port'] %>
}

# Remedy Gateway - Ticketing
[<%= node['prbsrv']['remedy'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['remedy_port'] %>
}

# Tivoli Data Warehouse Gateway - Reporting
[<%= node['prbsrv']['tdw'] %>]
{
  Primary: <%= node['OS'] %> <%= node['prbsrv']['tdw_port'] %>
}
