<?xml version='1.0' encoding='UTF-8'?>
<agent-input clean='true' temporary='true'>
  <variables>
    <variable name='sharedLocation' value='<%= node['objsrv']['app_dir'] %>/IBMIMShared'/>
    <!-- Installation location for: -->
    <!--   IBM Tivoli Netcool/OMNIbus -->
    <!--   Netcool/OMNIbus Gateway <%= node['objsrv']['gw_name'] %> -->
    <variable name='installLocation' value='<%= node['objsrv']['nc_dir'] %>'/>
  </variables>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
  <server>
    <repository location='file:<%= node['objsrv']['gw_dir'] %>/'/>
  </server>
  <profile id='IBM Tivoli Netcool OMNIbus' installLocation='${installLocation}'>
    <!-- Data for IBM Tivoli Netcool/OMNIbus: -->
    <!-- Migrate OMNIbus data -->
    <data key='user.migratedata,com.ibm.tivoli.omnibus.core' value='false'/>
  </profile>
  <install>
    <!-- Netcool/OMNIbus Gateway <%= node['objsrv']['gw_name'] %> <%= node['objsrv']['gw_ver'] %> -->
    <offering profile='IBM Tivoli Netcool OMNIbus' id='com.ibm.tivoli.omnibus.integrations.<%= node['objsrv']['gw_name'] %>' version='<%= node['objsrv']['gw_ver'] %>' features='<%= node['objsrv']['gw_name'] %>'/>
  </install>
</agent-input>
